{
  "name": "walletconnect",
  "version": "0.0.1-beta.2",
  "description":
    "Library to connect with wallet-connect bridge server for browsers and node.js",
  "main": "lib/library.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test":
      "mocha --require @babel/register --require babel-polyfill --colors ./test/*.spec.js",
    "test:watch":
      "mocha --require @babel/register --require babel-polyfill --colors -w ./test/*.spec.js",
    "lint": "eslint src/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maticnetwork/walletconnect.git"
  },
  "keywords": ["wallet", "react-native", "walletconnect", "react"],
  "author": "Jaynti Kanani <jdkanani@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maticnetwork/walletconnect/issues"
  },
  "homepage": "https://github.com/maticnetwork/walletconnect",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.13.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^4.0.1",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "@babel/register": "^7.0.0-beta.46",
    "frisbee": "^1.6.4",
    "xhr2": "^0.1.4"
  }
}
